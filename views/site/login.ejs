<!DOCTYPE html>
<html>

<head>
    <title>
        <%= dataSite.title %>
    </title>
    <%- include('includes/links-public'); -%>
</head>

<body>
    <div class="container">
        <div class="row">
            <div class="col-md-4 offset-md-4 col-sm-6 offset-sm-3">
                <div class="card mt-4">
                    <div class="card-header">
                        Login
                    </div>
                    <div class="card-body">
                        <form id="Login" method="post" action="/cemese/login">
                            <div class="form-group">
                                <input type="text" name="username" class="form-control" id="inputEmail" placeholder="Email Address">

                            </div>
                            <div class="form-group">
                                <input name="password" type="password" class="form-control" id="inputPassword"
                                    placeholder="Password">
                            </div>
                            <button type="submit" class="btn btn-primary btn-block">Login</button>
                        </form>
                    </div>
                    <div class="card-footer">
                        <a class="link" href="/cemese">Index</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        var exampleSocket = new WebSocket("ws://localhost:3000", "protocolOne");
    </script>
</body>

</html>